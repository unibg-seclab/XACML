<configuration>
	<appender name="CONSOLE" class="ch.qos.logback.core.ConsoleAppender">
		<encoder>
			<pattern>%d{HH:mm:ss.SS} [%thread] %-5level %logger{36} - %msg%n</pattern>
		</encoder>
	</appender>
	
	<logger name="eu.mosaicrown" level="DEBUG"/>
	
 	<root level="DEBUG">
		<appender-ref ref="CONSOLE" />
	</root>
</configuration>